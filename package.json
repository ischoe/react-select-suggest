{
  "name": "react-select-suggest",
  "version": "1.0.0",
  "description": "Auto-Suggest-Box built with React & Redux",
  "main": "lib/export.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ischoe/react-select-suggest"
  },
  "scripts": {
    "start": "npm run open:src",
    "open:src": "babel-node tools/srcServer.js",
    "test": "tape tools/testSetup.js -r babel-register src/ReactSelectSuggest/test/*.test.js --require ignore-styles",
    "test:watch": "tape-watch tools/testSetup.js -r babel-register src/ReactSelectSuggest/test/*.test.js --require ignore-styles",
    "build": "npm run build:lib",
    "build:lib": "babel src --out-dir lib --copy-files",
    "clean": "rimraf lib",
    "lint": "node_modules/.bin/esw src",
    "lint:watch": "npm run lint -- --watch",
    "check": "npm-run-all --parallel lint:watch test:watch"
  },
  "author": "Ingo Sch√∂nberger",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "6.24.1",
    "enzyme": "2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "3.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "11.0.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "2.4.3",
    "sinon": "^2.3.2",
    "tape": "4.6.3",
    "tape-watch": "^2.3.0",
    "watch": "0.17.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "css-loader": "0.28.4",
    "file-loader": "0.11.1",
    "ignore-styles": "^5.0.1",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-modal": "1.7.7",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-redux": "4.0.8",
    "react-unit": "3.0.0",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.18.1",
    "url-loader": "0.5.8"
  }
}
